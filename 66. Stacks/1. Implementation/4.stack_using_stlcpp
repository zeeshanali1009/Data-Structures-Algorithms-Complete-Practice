#include <iostream>
#include <stack>
using namespace std;

int main() {
    stack<int> st;

    // Push elements
    st.push(100);
    st.push(200);
    st.push(300);

    cout << "Top element: " << st.top() << endl; // 300
    st.pop();
    cout << "After pop, Top: " << st.top() << endl; // 200

    cout << "Stack size: " << st.size() << endl;
    cout << "Is stack empty? " << (st.empty() ? "Yes" : "No") << endl;

    return 0;
}
